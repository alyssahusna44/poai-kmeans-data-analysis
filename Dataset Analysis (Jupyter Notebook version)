    "# Implement K-Means with optimal K\n",
    "kmeans = KMeans(n_clusters=2, random_state=50)\n",
    "customer_data_scaled_df['Cluster'] = kmeans.fit_predict(customer_data_scaled_df[['Age', 'Spending Score (1-100)']])"
